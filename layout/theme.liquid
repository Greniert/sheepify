<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    {{ content_for_header }}
    <!-- Header hook for plugins -->
    <link href="
https://cdn.jsdelivr.net/npm/atropos@2.0.2/atropos.min.css
" rel="stylesheet">
    {{ 'app.css' | asset_url | stylesheet_tag }}
    <script src="{{ 'app.js' | asset_url }}" defer></script>
   
    <script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>

  </head>
  <body class="container mx-auto bg-[url('https://images.rawpixel.com/image_800/cHJpdmF0ZS9sci9pbWFnZXMvd2Vic2l0ZS8yMDIyLTA1L25zMTQ2NTEtaW1hZ2Uta3d2eWQybmIuanBn.jpg')] bg-cover bg-center bg-no-repeat py-5">
    {% section 'header' %}
    <main role="main">
      {{ content_for_layout }}
    </main>
  </body>
</html>